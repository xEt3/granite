<div class="ui form">
  <h3 class="ui header">General Account Settings</h3>
  <div class="ui divider"></div>

  {{quick-form
    settingsForm
    model
    this
    tagName='div'}}

  <div class="ui divider"></div>
  <form {{action "save" on="submit"}}>
    {{#form/action-button
      type="submit"
      class='ui positive fluid huge button'
      loading=working}}
      Save Changes
    {{/form/action-button}}
  </form>
</div>
