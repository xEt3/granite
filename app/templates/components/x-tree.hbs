{{#if hasBlock}}
  {{#x-tree-branch
    select=this.select
    hover=this.hover
    hoverOut=this.hoverOut
    checkable=this.checkable
    syncCheckbox=this.syncCheckbox
    chosenId=this.chosenId
    model=this.model as |node|}}
    {{yield node}}
  {{/x-tree-branch}}
{{else}}
  {{x-tree-branch
    model=this.model
    select=this.select
    hover=this.hover
    hoverOut=this.hoverOut
    checkable=this.checkable
    syncCheckbox=this.syncCheckbox
    chosenId=this.chosenId}}
{{/if}}
