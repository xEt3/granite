{{#if isEmployee}}
  {{display/employee-picture url=model.picture employeeId=model.id class="search-result-item__image"}}
{{/if}}
{{#if properties.title}}
  <div class="search-result-item__title">
    {{properties.title}}
  </div>
{{/if}}
{{#if properties.description}}
  <div class="search-result-item__description">
    {{properties.description}}
  </div>
{{/if}}
