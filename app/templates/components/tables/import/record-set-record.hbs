<td>
  {{#if this.duplicate}}
    <div class="ui red ribbon label import__duplicate-label">
      <EmberTooltip @side="right" @popperContainer="body">
        {{this.matchReason}} match
      </EmberTooltip>
      Possible duplicate of
      <a href="#" {{action "linkToDuplicate"}}>
        {{this.duplicate.text}}
      </a>
    </div>
  {{/if}}
  <div>
    <UiCheckbox @class="" @label="" @checked={{this.isSelected}} @onChange={{action this.onToggle}} />
  </div>
</td>
{{yield this.record}}
