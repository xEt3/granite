<div class="item">
  {{!-- {{#link-to "account.employee.index.education.certification" model.id class="item"}} --}}
  <div class="content">
    <h3 class="header">
      {{@model.name}}
    </h3>
    <div class="meta">
      {{#if @model.webinar}}
        Webinar: {{@model.webinar.webinar.title}}
      {{/if}}
