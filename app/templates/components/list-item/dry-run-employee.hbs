<div class="ui fluid card">
  <div class="content">
    <h3>
      Employee record #{{x-inc this.index}}
      {{#if this.missingRequiredFields}}
        <i class="exclamation circle icon text-red">
          {{ember-tooltip
            side="right"
            popperContainer="body"
            text=this.missingRequiredFields}}
        </i>
      {{/if}}
    </h3>
    <ul>
      {{#each-in this.dryRunRecord as |key value|}}
        {{census/dry-run-employee-relationship key=key value=value availableFields=this.availableFields}}
      {{/each-in}}
    </ul>
  </div>
</div>
