{
  "name": "granite",
  "version": "0.4.6",
  "description": "A solid solution for human resources",
  "private": true,
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "start": "ember server --proxy http://localhost:4000",
    "dock": "ember server --proxy http://$DOCKER_IP:$DOCKER_LB_PORT",
    "lint:js": "eslint ./*.js app config lib server tests",
    "test": "ember test",
    "push-image": "docker build -t james1x0/granite . && docker push james1x0/granite",
    "make-deployment": "bash scripts/deploy-eb.sh",
    "deploy": "npm run push-image && npm run make-deployment && eb deploy --staged"
